<h1>Pages#home</h1>
<p>gfrocrai</p>
<p>Now is <%= Time.now %></p>

<p>user info = <%= @user_info%></p>

<p>trimestre = <%= @trimestre%></p>

<%= column_chart @trimestre, library: {
	discrete: true,
	tooltip: {
		pointFormat: 'log: <b>{point.y}</b>',
		valueSuffix: 'h'
		},
} %>

<p>allinone = <%= @allinone%></p>

<%= column_chart @allinone, library: {
	discrete: true,
	tooltip: {
		pointFormat: 'log: <b>{point.y}</b>',
		valueSuffix: 'h'
		},
} %>

<p>yearbyyear = <%= @yearbyyear%></p>


<%= line_chart @yearbyyear.map { |k, v| {
		name: k, 
		data: v
	}
} %>


<% if @yearbyyear %>
<% @yearbyyear.each do |key, v_hash| %>
<%= line_chart v_hash, title: key, width: "800px" , label: "heures" %>
<%= v_hash %>
<% end %>
<% end %>
